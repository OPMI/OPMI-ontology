        <dc:subject xml:lang="en">An ontology for the annotation of the adverse event domain.</dc:subject>
        <dc:creator xml:lang="en">Bingjian Yang</dc:creator>
        <dc:creator xml:lang="en">Desikan Jagannathan</dc:creator>
